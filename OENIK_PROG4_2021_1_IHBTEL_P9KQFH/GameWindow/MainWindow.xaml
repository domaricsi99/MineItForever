<Window x:Class="GameWindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameControlerDll;assembly=GameControlerDll"
        xmlns:repo="clr-namespace:GameRepository;assembly=GameRepository"
        mc:Ignorable="d"
        Title="MainWindow" Width="913" Height="425"
        Closed="Window_Closed">
    

    <Window.Resources>
        <DataTemplate x:Key="Pickaxes" DataType="{x:Type repo:ShopRepsitory}">
            <StackPanel Orientation="Horizontal">
            </StackPanel>
        </DataTemplate>
    </Window.Resources>

    <Grid HorizontalAlignment="Center" Width="900" Height="420" VerticalAlignment="Top">
        <local:GameControl Margin="0,0,100,0" Height="366" VerticalAlignment="Top"/>

    </Grid>
</Window>
